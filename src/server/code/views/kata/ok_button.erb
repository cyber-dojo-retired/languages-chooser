<script>
'use strict';
$(() => {

  $('button#ok').click(() => {
    const exerciseName = encodeURIComponent('Fizz Buzz');
    const displayName = encodeURIComponent(cd.selectedDisplayName);
    const params = `exercise_name=${exerciseName}&language_name=${displayName}`;
    window.location = `<%= @create_url %>?${params}`;
  });

});
</script>

<div>
  <button type="button" id="ok">ok</button>
</div>
